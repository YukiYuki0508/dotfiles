[user]
	name = yukiIchige
	email = info@yuki-web.net
        editor = /usr/bin/vim
[core]
	editor = vim -c \"set fenc=utf-8\"
	quotepath = false
	precomposeunicode = true
	excludesfile = /Users/yukiendo/.gitignore
[color]
	diff = auto
	status = auto
	branch = auto
	ui = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/yukiendo/.stCommitMsg
